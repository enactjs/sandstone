{
  "name": "sandstone-sampler",
  "version": "3.0.0-alpha.4",
  "description": "Component and QA samples for Sandstone",
  "private": true,
  "main": "index.js",
  "scripts": {
    "bootstrap": "enact bootstrap",
    "clean": "enact clean",
    "cleanall": "enact clean --all",
    "lint": "enact lint --strict",
    "test": "echo \"No test suite\" && exit 0",
    "pack": "storybook build -c .storybook -o dist",
    "pack-qa": "storybook build -c .qa-storybook -o dist",
    "serve": "storybook dev -p 8080 -c .storybook --ci",
    "serve-qa": "storybook dev -p 8080 -c .qa-storybook --ci"
  },
  "license": "Apache-2.0",
  "enact": {
    "theme": "sandstone"
  },
  "dependencies": {
    "@enact/core": "^5.0.0-alpha.4",
    "@enact/i18n": "^5.0.0-alpha.4",
    "@enact/sandstone": "../../",
    "@enact/spotlight": "^5.0.0-alpha.4",
    "@enact/storybook-utils": "enactjs/storybook-utils#feature/eslint9",
    "@enact/ui": "^5.0.0-alpha.4",
    "@storybook/addon-docs": "^8.5.4",
    "@storybook/csf-tools": "^8.5.4",
    "@storybook/manager-api": "^8.5.4",
    "@storybook/react-webpack5": "^8.5.4",
    "@storybook/theming": "^8.5.4",
    "@storybook/test": "^8.5.4",
    "classnames": "^2.5.1",
    "ilib": "^14.20.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-ga4": "^2.1.0",
    "storybook": "^8.5.4"
  }
}
