{
  "name": "sandstone-sampler",
  "version": "1.0.0-alpha.6",
  "description": "Component and QA samples for Sandstone",
  "private": true,
  "main": "index.js",
  "scripts": {
    "bootstrap": "enact bootstrap",
    "clean": "enact clean",
    "cleanall": "enact clean --all",
    "lint": "enact lint --strict",
    "test": "echo \"No test suite\" && exit 0",
    "pack": "build-storybook -c .storybook -o dist",
    "pack-qa": "build-storybook -c .qa-storybook -o dist",
    "serve": "start-storybook -p 8080 -c .storybook --ci",
    "serve-qa": "start-storybook -p 8080 -c .qa-storybook --ci"
  },
  "license": "Apache-2.0",
  "enact": {
    "theme": "sandstone"
  },
  "dependencies": {
    "@enact/core": "^3.4.9",
    "@enact/i18n": "^3.4.9",
    "@enact/sandstone": "enyojs/sandstone#1.4.4",
    "@enact/spotlight": "^3.4.9",
    "@enact/ui": "^3.4.9",
    "classnames": "^2.2.6",
    "ilib": "^14.4.0",
    "query-string": "^6.7.0",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@enact/storybook-utils": "https://github.com/enactjs/storybook-utils#feature/storybook_migration_to_v6",
    "@storybook/addons": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@storybook/theming": "^6.1.11",
    "babel-loader": "^8.2.2",
    "core-js": "^3.8.1"
  }
}
