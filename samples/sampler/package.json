{
  "name": "sandstone-sampler",
  "version": "2.0.2",
  "description": "Component and QA samples for Sandstone",
  "private": true,
  "main": "index.js",
  "scripts": {
    "bootstrap": "enact bootstrap",
    "clean": "enact clean",
    "cleanall": "enact clean --all",
    "lint": "enact lint --strict",
    "test": "echo \"No test suite\" && exit 0",
    "pack": "build-storybook -c .storybook -o dist",
    "pack-qa": "build-storybook -c .qa-storybook -o dist",
    "serve": "start-storybook -p 8080 -c .storybook --ci",
    "serve-qa": "start-storybook -p 8080 -c .qa-storybook --ci"
  },
  "license": "Apache-2.0",
  "enact": {
    "theme": "sandstone"
  },
  "dependencies": {
    "@enact/core": "^4.1.1",
    "@enact/i18n": "^4.1.1",
    "@enact/sandstone": "../../",
    "@enact/spotlight": "^4.1.1",
    "@enact/ui": "^4.1.1",
    "classnames": "^2.2.6",
    "ilib": "^14.4.0",
    "query-string": "^6.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@enact/storybook-utils": "^4.1.0",
    "@storybook/addons": "6.3.12",
    "@storybook/react": "6.3.12",
    "@storybook/theming": "6.3.12",
    "babel-loader": "^8.2.2",
    "core-js": "^3.8.1"
  }
}
