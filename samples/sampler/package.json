{
  "name": "sandstone-sampler",
  "version": "2.7.2",
  "description": "Component and QA samples for Sandstone",
  "private": true,
  "main": "index.js",
  "scripts": {
    "bootstrap": "enact bootstrap",
    "clean": "enact clean",
    "cleanall": "enact clean --all",
    "lint": "enact lint --strict",
    "test": "echo \"No test suite\" && exit 0",
    "pack": "storybook build -c .storybook -o dist",
    "pack-qa": "storybook build -c .qa-storybook -o dist",
    "serve": "storybook dev -p 8080 -c .storybook --ci",
    "serve-qa": "storybook dev -p 8080 -c .qa-storybook --ci"
  },
  "license": "Apache-2.0",
  "enact": {
    "theme": "sandstone"
  },
  "dependencies": {
    "@enact/core": "^4.7.1",
    "@enact/i18n": "^4.7.1",
    "@enact/sandstone": "../../",
    "@enact/spotlight": "^4.7.1",
    "@enact/storybook-utils": "enactjs/storybook-utils#feature/WRP-569",
    "@enact/ui": "^4.7.1",
    "@storybook/addon-docs": "^7.1.0",
    "@storybook/addons": "^7.1.0",
    "@storybook/react": "^7.1.0",
    "@storybook/react-webpack5": "^7.1.0",
    "@storybook/theming": "^7.1.0",
    "babel-loader": "^8.2.5",
    "classnames": "^2.3.2",
    "ilib": "^14.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.1.0"
  }
}
