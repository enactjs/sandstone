{
	"name": "qa-videoplayer",
	"version": "1.0.0",
	"description": "A sample with a VideoPlayer that supports timeline selection.",
	"author": "",
	"main": "src/index.js",
	"scripts": {
		"serve": "enact serve",
		"pack": "enact pack",
		"pack-p": "enact pack -p",
		"watch": "enact pack --watch",
		"clean": "enact clean",
		"lint": "enact lint --strict .",
		"test": "enact test start --single-run --browsers PhantomJS",
		"test-json": "enact test start --single-run --browsers PhantomJS --reporters json",
		"test-watch": "enact test start --browsers PhantomJS"
	},
	"license": "Apache-2.0",
	"private": true,
	"repository": "https://github.com/enyojs/enact-internal-samples",
	"enact": {
		"theme": "sandstone",
		"title": "QA VideoPlayer",
		"isomorphic": true,
		"ri": {
			"baseSize": 24
		}
	},
	"eslintConfig": {
		"extends": "enact/strict"
	},
	"eslintIgnore": [
		"node_modules/*",
		"build/*",
		"dist/*"
	],
	"dependencies": {
		"@enact/core": "^3.3.0-alpha.5",
		"@enact/i18n": "^3.3.0-alpha.5",
		"@enact/sandstone": "enyojs/sandstone",
		"@enact/spotlight": "^3.3.0-alpha.5",
		"@enact/ui": "^3.3.0-alpha.5",
		"ilib": "^14.2.0",
		"prop-types": "^15.7.2",
		"react": "^16.8.6",
		"react-dom": "^16.8.6"
	}
}